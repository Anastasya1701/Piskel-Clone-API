{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-alert": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "max-classes-per-file": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "no-undef": 0,
    "no-useless-concat": 0
  }
}
